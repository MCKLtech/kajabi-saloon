{
  "name": "mckltech/kajabi-saloon",
  "description": "PHP Library for the Kajabi API using Saloon - Compatible with Thinkific Saloon responses",
  "authors": [
    {
      "name": "Colin Longworth",
      "email": "info@wooninja.io"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/MCKLtech/lms-contracts"
    }
  ],
  "require": {
    "php": "^8.1",
    "saloonphp/saloon": "^3.0",
    "saloonphp/pagination-plugin": "^2.2",
    "saloonphp/rate-limit-plugin": "^2.0",
    "nesbot/carbon": "*",
    "mckltech/lms-contracts": "dev-main"
  },
  "require-dev": {
    "spatie/ray": "*",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "WooNinja\\KajabiSaloon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable" : true
}
